{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "deviceId": { "type": "string" },
          "host-name": { "type": "string" },
          "system-ip": { "type": "string" },
          "device-type": { "type": "string" }
        },
        "required": ["deviceId", "host-name", "system-ip", "device-type"]
      }
    }
  },
  "required": ["data"]
}
